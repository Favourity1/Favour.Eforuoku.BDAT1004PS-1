    "def triangleArea(a,b,c):\n",
    "\n",
    " s= ((a+b+c)/2)\n",
    "\n",
    " area = math.sqrt(s*(s-a)*(s-b)*(s-c))\n",
    "\n",
    " return area\n",
    "\n",
    "\n",
    "\n",
    "print(triangleArea(2,2,2))"
   ]
